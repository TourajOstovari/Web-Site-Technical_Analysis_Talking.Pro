
<!DOCTYPE html>
<html>
@using Technical_Analysis_Talking.Pro
@model PagedList.IPagedList<Technical_Analysis_Talking.Pro.App_Data.Posts_>
@using PagedList.Mvc
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="generator" content="Responsive Site Designer 2.5.2425">
    <title>Archive of my analyst || FOREX Trader || Forex Analysis</title>
    <link rel="stylesheet" href="~/css/bootstrap4.min.css">
    <link rel="stylesheet" href="~/css/wireframe-theme.min.css">
    <script>document.createElement("picture");</script>
    <script class="picturefill" async="async" src="~/js/picturefill.min.js"></script>
    <link rel="stylesheet" href="~/css/main.css">
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Average+Sans%7CQuattrocento%7CQuattrocento+Sans:400,i%7CQuintessential%7CRaleway:200,400,b,i%7CRambla:400,i%7CYellowtail">
</head>

<body>
    <div class="row row-1">
        <div class="col-xs-12 col-md-11 col-sm-12 custom-320-col-xs-12 col-lg-10">
            <h6 class="heading-3">
                <a title="" href="" class="heading-text-2">Welcome To My Technical Analysis Blog</a>
            </h6>
        </div>
        <div class="col-xs-7 col-lg-2 custom-320-col-xs-6">
            <h6>
                <a title="With your donates this website would be continue and useful" href=""><code>Donate us</code></a>
            </h6>
            <h6>
                <code><a title="" href="@Url.Action("Index", "Home")">Home Page</a></code>
            </h6>
        </div>
    </div>
