            <div class="col-xs-4">
                <h6>
                    <span class="heading-text-7">Categories (Symbols):</span><br>
                </h6>
            </div>
            <div class="col-xs-12">
                <a class="link-text text-info" href="#" target="_blank">
                    @{

                        Technical_Analysis_Talking.Pro.App_Data.Database_StructContainer database_ = new Technical_Analysis_Talking.Pro.App_Data.Database_StructContainer();
