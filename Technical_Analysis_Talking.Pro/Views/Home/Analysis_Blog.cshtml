        </div>*@
    <div class="row">
        @{
            foreach (Technical_Analysis_Talking.Pro.App_Data.Posts_ item in Model)
            {
                <div class="col-xs-12">
                    <div class="subgrid">
                        <div class="row">
                            <div class="col-xs-12 subgrid-column-1">
                                <h6>
                                    <a title="Title: alt" href="" target="_blank" class="heading-text-5"><em class="heading-text-6">@item.title: </em></a>
                                </h6>
                            </div>
                        </div>
                        <div class="row subgrid-row-1">
                            <div class="col-xs-12 subgrid-column-3 col-lg-12">
                                <div class="responsive-picture picture-1 img-thumbnail">
                                    <picture>
                                        <!--[if IE 9]><video style="display: none;"><![endif]-->
                                        @*<source srcset="~/images/EURUSD%20-%20Primary%20Analysis%20-%20Feb-14%201336%20PM%20%281%20day%29.png">*@
                                        <source srcset="~/images/@item.image" media="(min-width: 992px)">
                                        <source srcset="~/images/@item.image" media="(min-width: 768px)">
                                        <source srcset="~/images/@item.image" media="(min-width: 576px)">
                                        <source srcset="~/images/@item.image" media="(min-width: 320px)"><!--[if IE 9]></video><![endif]-->
                                        <img alt="... Analysis Photo Not Found ..." src="~/images/@item.image">
                                    </picture>
                                </div>
                            </div>
                            <div class="col-xs-11 custom-320-col-xs-6 col-sm-6 col-lg-4">
                                <h6 class="heading-4">
                                    <a title="" href=""><span class="heading-text-4">Date: </span><em>@item.date</em></a>
                                </h6>
                            </div>
                            <div class="col-xs-12 col-lg-7">
                                <h6><a title="" href="" class="heading-text-3">Authore:</a>&nbsp;@item.authore</h6>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-xs-12 pull-lg-0 col-lg-3 push-lg-9">
                                <a href="@String.Concat(Url.Action("Viewpost","Home"),@"/?id=",item.Id)"><button type="button" class="btn btn-danger bg-info">More ...</button></a>
                            </div>
                        </div>
                    </div>
                </div>
            }
        }

    </div>
    <ul class="pagination pagination-lg"  style="margin-left:30em;">
        Page 
            @for (int i = 1; i <= Model.PageCount; i++)
            {
            <span style="margin-left:20px"><a href="@string.Concat(Url.Action("Archive","Home"),"?Page=",i) ">@i</a></span>
            }
    </ul>
    <div class="row">
        <div class="col-xs-12">
            <footer class="container-fluid">
                <div class="placeholder-hero hero-unit-1">
                    <h1 class="hero-header">
                        <span class="hero-header-text-1">Technical Forex Market News. BE FIRST IN MARKET WITH US... &nbsp;<span class="hero-header-text-2">BY Touraj Ostovari</span></span>
                    </h1>
                    <p class="hero-tagline hero-tagline-1">
                        Designed &amp; Developed by <span class="hero-tagline-text-1">Mr. Touraj Ostovari</span><br>
                    </p>
                    <p class="hero-tagline hero-tagline-1">
